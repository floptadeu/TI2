package ti2;

public class Usuario {
	private String email;
	private String senha;
	private String nome;
	private String empresa;
	private String usuario;	
	private int cpf;
	private int cnpj;
	
	public Usuario() {
	email = "";
	senha = "";
	nome ="";
	empresa	=""; 
	usuario	="";
	cpf	= -1;
	cnpj = -1;
		
		
	}
	public Usuario(String email,String senha,String nome,String empresa,String usuario,int cpf,int cnpj) {
	this.email = email;
	this.senha = senha;
	this.nome	= nome;
	this.empresa = empresa;
	this.usuario = usuario;
	this.cpf = cpf;
	this.cnpj = cnpj;
	}
	@Override
	public String toString() {
		return "Usuario [email=" + email + ", senha=" + senha + ", nome=" + nome + ", empresa=" + empresa + ", usuario="
				+ usuario + ", cpf=" + cpf + ", cnpj=" + cnpj + "]";
	}
	
	public String getEmail() {
		return email;
	}
	public String getSenha() {
		return senha;
	}
	public String getNome() {
		return nome;
	}
	public String getEmpresa() {
		return empresa;
	}
	public String getUsuario() {
		return usuario;
	}
	public int getCpf() {
		return cpf;
	}
	public int getCnpj() {
		return cnpj;
	}
	
	
	public void setEmail(String email) {
		this.email = email;
	}
	public void setSenha(String senha) {
		this.senha = senha;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public void setEmpresa(String empresa) {
		this.empresa = empresa;
	}
	public void setUsuario(String usuario) {
		this.usuario = usuario;
	}
	public void setCpf(int cpf) {
		this.cpf = cpf;
	}
	public void setCnpj(int cnpj) {
		this.cnpj = cnpj;
	}
	
}
