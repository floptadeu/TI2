package ti2;

import java.time.LocalDate;

public class Estoque {
	private int codPro;
	private float valor;
	private String descricao;
	private LocalDate dataVal;
	private int posicao;
	private int armazem;
	private String categoria;
	private int prateleira;
	private int quantidade;
	public Estoque(int codPro, float valor, String descricao, LocalDate dataVal, int posicao, int armazem,
			String categoria, int prateleira, int quantidade) {
		super();
		this.codPro = codPro;
		this.valor = valor;
		this.descricao = descricao;
		this.dataVal = dataVal;
		this.posicao = posicao;
		this.armazem = armazem;
		this.categoria = categoria;
		this.prateleira = prateleira;
		this.quantidade = quantidade;
	}
	@Override
	public String toString() {
		return "estoque [codPro=" + codPro + ", valor=" + valor + ", descricao=" + descricao + ", dataVal=" + dataVal
				+ ", posicao=" + posicao + ", armazem=" + armazem + ", categoria=" + categoria + ", prateleira="
				+ prateleira + ", quantidade=" + quantidade + "]";
	}
	public int getCodPro() {
		return codPro;
	}
	public float getValor() {
		return valor;
	}
	public String getDescricao() {
		return descricao;
	}
	public LocalDate getDataVal() {
		return dataVal;
	}
	public int getPosicao() {
		return posicao;
	}
	public int getArmazem() {
		return armazem;
	}
	public String getCategoria() {
		return categoria;
	}
	public int getPrateleira() {
		return prateleira;
	}
	public int getQuantidade() {
		return quantidade;
	}
	public void setCodPro(int codPro) {
		this.codPro = codPro;
	}
	public void setValor(float valor) {
		this.valor = valor;
	}
	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}
	public void setDataVal(LocalDate dataVal) {
		this.dataVal = dataVal;
	}
	public void setPosicao(int posicao) {
		this.posicao = posicao;
	}
	public void setArmazem(int armazem) {
		this.armazem = armazem;
	}
	public void setCategoria(String categoria) {
		this.categoria = categoria;
	}
	public void setPrateleira(int prateleira) {
		this.prateleira = prateleira;
	}
	public void setQuantidade(int quantidade) {
		this.quantidade = quantidade;
	}
	
}
